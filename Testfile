int[] intArray = {5, 3, 8, 4, 2};
        System.out.println("Original Integer Array: ");
        for (int num : intArray) {
            System.out.print(num + " ");
        }
        System.out.println("\nSorted Integer Array: ");
        intArray = sorter.quickSortIntegers(intArray);
        for (int num : intArray) {
            System.out.print(num + " ");
        }
